{
  "name": "Chrome Session Tracker",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["storage", "alarms"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "timer.html",
    "default_icon": "clock.png"
  }
}
